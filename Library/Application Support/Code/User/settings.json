    "C_Cpp.loggingLevel": "Debug",
    "cmake.cmakePath": "/usr/local/bin/cmake",
    "cmake.configureOnOpen": false,
    "debug.internalConsoleOptions": "openOnSessionStart",
    "diffEditor.ignoreTrimWhitespace": false,
