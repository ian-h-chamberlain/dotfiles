        "editorCursor.foreground": "#9d550f",
        "editor.selectionBackground": "#9d550f"
    },
    "explorer.autoReveal": false,
    "terminal.integrated.fontSize": 11,
    "terminal.integrated.scrollback": 10000,
    "editor.fontSize": 11,
