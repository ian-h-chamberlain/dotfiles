    // vim settings
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.iskeyword": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-_",
    "vim.normalModeKeyBindings": [
        {
            "after": [
                "<esc>"
            ],
            "before": [
                "<esc>"
            ],
            "commands": [
                {
                    "command": ":nohl"
                },
                {
                    "command": "closeFindWidget",
                    "key": "escape",
                    "when": "findWidgetVisible"
                },
                {
                    "command": "workbench.action.closePanel",
                    "key": "escape"
                },
                {
                    "command": "notifications.hideToasts",
                    "when": "notificationToastsVisible"
                },
                {
                    "command": "notifications.hideList",
                    "when": "notificationCenterVisible"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "?"
            ],
            "commands": [
                {
                    "command": "actions.find",
                    "when": "editorFocus"
                },
                {
                    "command": "editor.action.previousMatchFindAction",
                    "when": "editorFocus"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "/"
            ],
            "commands": [
                {
                    "command": "actions.find",
                    "when": "editorFocus"
                },
                {
                    "command": "editor.action.nextMatchFindAction",
                    "when": "editorFocus"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "n"
            ],
            "commands": [
                {
                    "command": "editor.action.nextMatchFindAction",
                    "when": "editorFocus"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "N"
            ],
            "commands": [
                {
                    "command": "editor.action.previousMatchFindAction",
                    "when": "editorFocus"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "u"
            ],
            "commands": [
                {
                    "args": [],
                    "command": "undo"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "<C-r>"
            ],
            "commands": [
                {
                    "args": [],
                    "command": "redo"
                }
            ]
        }
    ],
    "vim.searchHighlightColor": "rgba(157, 85, 15, 1.0)",
