    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<esc>"
            ],
            "after": [
                "<esc>"
            ],
            "commands": [
                {
                    "command": ":nohl"
                },
                {
                    "command": "closeFindWidget",
                    "when": "findWidgetVisible"
                },
                {
                    "command": "workbench.action.closePanel"
                }
            ]
        },
        {
            "before": [
                "?"
            ],
            "after": [],
            "commands": [
                {
                    "command": "actions.find",
                    "when": "editorFocus"
                },
                {
                    "command": "editor.action.previousMatchFindAction",
                    "when": "editorFocus"
                }
            ]
        },
        {
            "before": [
                "/"
            ],
            "after": [],
            "commands": [
                {
                    "command": "actions.find",
                    "when": "editorFocus"
                },
                {
                    "command": "editor.action.nextMatchFindAction",
                    "when": "editorFocus"
                }
            ]
        },
        {
            "before": [
                "n"
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.nextMatchFindAction",
                    "when": "editorFocus"
                }
            ]
        },
        {
            "before": [
                "N"
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.previousMatchFindAction",
                    "when": "editorFocus"
                }
            ]
        },
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo",
                    "args": []
                }
            ]
        }
    ],
